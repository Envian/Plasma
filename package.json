{
  "name": "Plasma",
  "main": "./lib/plasma",
  "version": "0.2.3",
  "description": "A standalone plugin to develop Salesforce Applications.",
  "keywords": [
    "salesforce",
    "force.com",
    "salesforce.com",
    "plasma",
    "apex",
    "visualforce",
    "lightning",
    "aura"
  ],
  "repository": "https://github.com/Envian/Plasma",
  "homepage": "https://github.com/Envian/Plasma",
  "bugs": {
    "url": "https://github.com/Envian/Plasma"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "keytar": ">=4.0.4",
    "etch": ">=0.12.6",
    "jszip": ">=3.1.5",
    "opn": ">=0.5.2"
  },
  "devDependencies": {
    "typescript": "latest",
    "tslib": "latest",
    "@types/node": "latest",
    "@types/atom": "latest",
    "@types/keytar": "latest",
    "@types/jszip": "latest",
    "@types/opn": "latest"
  },
  "package-deps": {
    "language-salesforce": ">=0.6.0"
  },
  "deserializers": {
